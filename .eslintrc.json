{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-console": 0,
    "global-require": 0,
    "import/no-unresolved": 0,
    "import/no-dynamic-require": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "import/extensions": 0,
    "import/newline-after-import": 0,
    "no-multi-assign": 0,
    "no-debugger": 0,
    "no-underscore-dangle": 0
  },
  "globals": {
    "DEBUG": false,
    "TARGET": false,
    "VERSION": false,
    "CONFIG": false
  }
}